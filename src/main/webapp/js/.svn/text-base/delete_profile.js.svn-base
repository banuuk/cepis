function setNext(args)
{
	if(args == "program"){
		document.delete_form.action="delete_program_data.cfm";
	} else if( args == "college"){
		document.delete_form.action="delete_college_data.cfm";	
	} else if( args == "personal"){
		document.delete_form.action="delete_personal_data.cfm";	
	} else if( args == "computer"){
		document.delete_form.action="delete_computer_data.cfm";	
	} else if( args == "phone"){
		document.delete_form.action="delete_phone_data.cfm";	
	} else if( args == "address"){
		document.delete_form.action="delete_address_data.cfm";	
	}
	document.delete_form.prev.value="0";
	document.delete_form.next.value="1";
	document.form.focus();
	document.delete_form.submit();

}

function setPrev(args)
{
	if(args == "program")
		document.delete_form.action="delete_program_data.cfm";
	else if( args == "college")
		document.delete_form.action="delete_college_data.cfm";	
	else if( args == "personal")
		document.delete_form.action="delete_personal_data.cfm";	
	else if( args == "computer")
		document.delete_form.action="delete_computer_data.cfm";	
	else if( args == "phone")
		document.delete_form.action="delete_phone_data.cfm";	
	else if( args == "address")
		document.delete_form.action="delete_address_data.cfm";	

	document.delete_form.prev.value="1";
	document.delete_form.next.value="0";
	document.form.focus();
	document.delete_form.submit();
}

