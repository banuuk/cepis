body {
	background-color:#000066;
	text-align: center;
}

#bhead {
	margin-left:0%;
	margin-right:0%;
	background-image: url(http://www.uky.edu/Education/nav/blend72sansline.jpg);
	height: 97px;
	text-align: center;
	font-family:Arial, Helvetica, sans-serif;
	color: white;
	font-weight:bold;
	font-size: 18pt;
}

#buser {
	text-align: center;
	font-family:Arial, Helvetica, sans-serif;
	font-weight:bold;
	font-size: 10pt;
	margin-top: 10px;
	margin-bottom: 10px;
}

#ws {
	width: 98%;
	margin-bottom: 5px;
	background-color:#FFFFFF;
	text-align: left;
	margin-left: auto;
	margin-right: auto;
}

#viewPane{
	text-align: center;
	font-weight: bold;
	font-size: 12px;
	color: #000066;
	border-bottom: 1px solid #CCCCCC;
	margin-bottom: 5px;
	padding-bottom: 5px;
}

#wspace{
	background-color:#FFFFFF;
	padding: 15px;
	padding-top: 1px;
}

#main {
	background-color:#FFFFFF;
	margin-top:30px;
	margin-left:2%;
	margin-right:2%;
}

.heading {
	text-align: center;
	font-weight: bold;
	font-size: 16px;
	color: #000066;
	border-bottom: 1px solid #CCCCCC;
	margin-bottom: 5px;
	padding-bottom: 5px;
}
#footer {
	background-color: white;
	margin-top: 20px;
	margin-bottom: 20px;
	margin-left: auto;
	margin-right: auto;
	width: 98%;
	text-align:center;
	font-family:Arial, Helvetica, sans-serif;
	font-size: 8pt;
	font-weight:normal;
}
