package com.mytutorial;

/**
 * @author Keerthi
 * 
 */
public class PraxisRangePrinter {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int s1 = 8, s2 = 4, s3 = 3, s4 = 1, s5 = 1, s6 = 1;
		int index = 140;
		for (int i = 0; i < 16; i++) {
			System.out.print(index + 1);
			System.out.print("-");
			index = index + s1;
			System.out.print(index + ", ");
			System.out.print(index + 1);
			System.out.print("-");
			index = index + s2;
			System.out.print(index + ", ");
			System.out.print(index + 1);
			System.out.print("-");
			index = index + s3;
			System.out.print(index + ", ");
			System.out.print(index + 1);
			System.out.print("-");
			index = index + s4;
			System.out.print(index + ", ");
			System.out.print(index + 1);
			System.out.print("-");
			index = index + s5;
			System.out.print(index + ", ");
			System.out.print(index + 1);
			System.out.print("-");
			index = index + s6;
			System.out.print(index + ", ");
		}

	}

}
